<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.am2019.alkomaster">

    <application
            android:allowBackup="false"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activities.comparator_history.ComparatorHistoryActivity" android:parentActivityName=".activities.ComparatorActivity">
        </activity>
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".breathalyser.LevelActivityData" android:parentActivityName=".activities.MainActivity" >
    </activity>

        <activity android:name=".breathalyser.AlcoholLevelAlcohols" android:parentActivityName=".breathalyser.LevelActivityData">
        </activity>

        <activity android:name=".breathalyser.Results" android:parentActivityName=".breathalyser.LevelActivityData">
        </activity>
        <activity android:name=".activities.ComparatorActivity" android:parentActivityName=".activities.MainActivity"/>
        <activity android:name=".breathalyser.CalculatorHistory" />

    </application>

</manifest>
